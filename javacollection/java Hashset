import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        sc.nextLine();
        String b;
       HashSet<String>  hs = new HashSet<> ();
        for( int i= 0;i<n ;i++){
         
        b = sc.nextLine();
            hs.add(b);
            System.out.println(hs.size());
        }
    }
}
